# Where Clawdbot Gateway is listening (same host/port as WS)
CLAWDBOT_GATEWAY_URL=http://127.0.0.1:18789

# Gateway auth token/password (depending on your gateway.auth.mode)
CLAWDBOT_GATEWAY_TOKEN=change_me

# Which agent id to run (default main)
CLAWDBOT_AGENT_ID=main

# Local port for this bridge server
PORT=3210
